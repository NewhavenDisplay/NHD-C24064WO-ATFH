void Write_command(unsigned char command){
  digitalWrite(LCM_CS1,LOW);          //LCM_CS1 = 0;
  digitalWrite(LCM_CS2,LOW);          //LCM_CS2 = 0;
  digitalWrite(LCM_RS,LOW);           //LCM_RS = 0;
  digitalWrite(LCM_RW,LOW);           //LCM_RW = 0;
  write_8bit(command);                //LCM_PORT
  digitalWrite(LCM_ENABLE,HIGH);      //LCM_ENABLE = 1;
  delayMicroseconds(10);
  digitalWrite(LCM_ENABLE,LOW);       //LCM_ENABLE = 0;
}

