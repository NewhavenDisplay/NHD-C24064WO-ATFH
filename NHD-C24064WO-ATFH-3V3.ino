void Initial_ST7565(){
  digitalWrite(LCM_PS1,HIGH);  //Parallel
  digitalWrite(LCM_C86,HIGH);  //6800
  Write_command(0xE2);    //RESET
  Write_command(0xA1);    //SET ADC=0 S1 to S128    A1
  Write_command(0xC0);    //SET SHL=0 C1 to C64 . C0
  Write_command(0xA2);    //SET BIAS=1 1/9 Bias, 1/65 Duty
  Write_command(0x2F);    //POWER control con.,reg.,follower ON
  Write_command(0x27);    //Ref. R select 00100--- (max:25)
  Write_command(0x81);    //Set Voltage Ref. Mode
  Write_command(0x20);      //LEVEL 32 xx------0x25  VOP (max:32)
  Write_command(0x40);    //Initial Display Line  01------   40
  Write_command(0xB0);    //Set Page From 0     80
  Write_command(0x10);    //Set Column Address MSB from 00
  Write_command(0x00);    //Set Column Address LSB from 00
  Write_command(0xAF);    //Set Display ON
  Write_command(0xA6);
}

void Init_Display(){
  Initial_ST7565();
  ClearDisplay();
 
}

